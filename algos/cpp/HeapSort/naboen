题目3：
#include <iostream>
using namespace std;

vector<int> func3(int m[20], int k){
    map<int, int> map1;
    for(int i=0;i< sizeof(m)/sizeof(int);i++){
        if(map1.find(k-m[i] != map1.end()){
            return vector<int>{map1.find(k-m[i])->second, i}
        }
        map1.insert({nums[i], i});
    }
    return return vector<int>{-1, -1};
}


题目2：
#include <iostream>
using namespace std;

int func2(){
    int T;
    cin>>T;
    while(T--){
        int m, n;
        cin >>m >> n;
        int above = 0, below = 0;

        for (int i = n+1; i<=m;i++){
            int num = i;
            while(num%2==0){
                num = num / 2;
                above++;
            }
        }

        for (int j = 1; i<=m-n;j++){
            int num = j;
            while(num%2==0){
                num = num / 2;
                below++;
            }
        }
        printf("%d\n", above - below);
    }
}




